<!-- 
RenameBlankNodes - Find nodes with no name and give them a name
-----
RenameBlankNodes finds nodes that do not have a name and creates a name for them.
 -->
<Config>
    <!-- 
    a - Description and repercussions
    -----
    Possible Values:
    b - Meaning of b
    c - Meaning of c
     -->
    <Param name="a">b</Param>

    <!-- 
    ModelDestination - The Jena model in which to do the rename.
     -->
    <Param name="a">b</Param>

    <!-- 
    ModelDedup - Model which contains triples whose names we do not want to create duplicates of in the destination model.
     -->
    <Param name="a">b</Param>

    <!-- 
    BaseNamespace - The first part of the namespace we wish to give to the renamed nodes.
     -->
    <Param name="a">b</Param>

    <!-- 
    AddNamespace - The part of the namespace between the base and the ID number
     -->
    <Param name="a">b</Param>

    <!-- 
    PredicateNamespace - The namespace for predicates.
     -->
    <Param name="a">b</Param>

</Config>